    "discord.lowerDetailsIdling": "Editing",
    "discord.lowerDetailsEditing": "at {file_name} ({file_size})",
    "discord.detailsIdling": "Idle...",
    "discord.smallImage": "this.state.discord.smallStatus",
    "discord.largeImageIdling": "ðŸ˜´ðŸ˜´ðŸ˜´",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.fontSize": 18,
    "editor.cursorStyle": "block", // this is the annoying cursor :wink:
    "editor.cursorSmoothCaretAnimation": true, // this is also kind of annoying haha
    "editor.lineNumbers": "relative", // relative line counting
    "terminal.integrated.cursorStyle": "block",
    "editor.fontFamily": " 'JetBrains Mono', 'Fira Code', Hasklig, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "backgroundCover.imagePath": "c:\\background\\856978.jpg",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[blade]": {
        // https://marketplace.visualstudio.com/items?itemName=onecentlin.laravel-blade
        // Laravel Blade Snippets
        "editor.autoClosingBrackets": "always"
    },
    // "redhat.telemetry.enabled": true,
    // "discord.removeRemoteRepository": true,
    // "terminal.integrated.cursorStyle": "line",
    "security.workspace.trust.untrustedFiles": "open",
    "blade.format.enable": true,
    "phpSniffer.autoDetect": true,
    "emmet.triggerExpansionOnTab": true,
    "phpSniffer.snippetExcludeSniffs": [
        {
            "[php]": {
              "editor.defaultFormatter": "wongjn.php-sniffer"
            }
        },
    ],
    "oneDarkPro.vivid": true,
    "oneDarkPro.bold": true,
    "oneDarkPro.editorTheme": "oneDarkProFlat",
    "remote.SSH.remotePlatform": {
        "65.21.109.149": "linux"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "backgroundCover.opacity": 1,
    "workbench.colorTheme": "Bearded Theme Vivid Black",
    "python.insidersChannel": "weekly",
    "discord.detailsEditing": "Editing {workspace}",
    "cmake.configureOnOpen": true,
    "redhat.telemetry.enabled": true,
    "bracketPairColorizer.depreciation-notice": false,
    "editor.autoClosingBrackets": "always",
    "window.zoomLevel": 2,
    "jupyter.askForKernelRestart": false,

    // rustc
    "editor.tokenColorCustomizations": {
        "[Theme Name]": {
            "textMateRules": [
                {
                    "scope": "variable.other.rust",
                    "settings": {
                        "foreground": "#ffff00"
                    }
                }
            ]
        }
    },
    "go.toolsManagement.autoUpdate": true,
    "editor.glyphMargin": true,
